<div class="container row justify-content-center">
  <div class="col-4 ">
    <h1 class="m-5">My Lists</h1>
  </div>
  <div class="d-flex justify-content-center m-4">
      <% @lists.each do |list| %>
        <%= link_to list.name, list_path(list), class: "col-3 btn card border border-secondary m-3" %>
      <% end %>
  </div>
</div>
